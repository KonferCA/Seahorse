{
    "version": 2,
    "buildCommand": "pnpm build",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "regions": ["sfo1"],
    "env": {
      "NEXT_PUBLIC_NETWORK_ID": "testnet"
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ],
    "outputDirectory": "frontend"
  }